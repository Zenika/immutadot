{
  "presets": [
    ["env", {
      "modules": "umd"
    }],
    ["stage-2"],
  ],
  "env": {
    "production": {
      "ignore": ".spec.js",
      "plugins": [
        ["module-resolver", {
          "root": ["./tmpsrc"]
        }]
      ],
    },
    "test": {
      "plugins": [
        ["module-resolver", {
          "root": ["./src", "./generated", "../../misc"]
        }]
      ]
    }
  }
}
